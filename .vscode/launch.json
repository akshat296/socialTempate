{   
    // Change WorkSpace and skipFiles name Accordingly
    // move this file in the outer directory of ./vscode/launch.json
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Server-Backend",
            "program": "workspaceRoot/snake-backend-api/server.js",
            "skipFiles": [
                "workspaceRoot/snake-backend-api/node_modules/**/**/*.js",
                "<node_internals>/**/*.js",
                "workspaceRoot/snake-backend/lib/**/*.js"
            ]
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Client",
            "url": "http://localhost:3000",
            "webRoot": "workspaceFolder",
            "skipFiles": [
                "workspaceRoot/social-frontend/node_modules/**/*.js",
                "<node_internals>/**/*.js",
                "workspaceRoot/social-frontend/lib/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Server",
            "program": "workspaceFolder/social-frontend/index.js",
            "skipFiles": [
                "workspaceRoot/social-frontend/node_modules/**/**/*.js",
                "<node_internals>/**/*.js",
                "workspaceRoot/social-frontend/lib/**/*.js"
            ]
        }
    ],
    "compounds": [
        {
            "name": "Full-Stack",
            "configurations": [
                "Client",
                "Server",
                "Server-Backend"
               
            ]
        }
    ]
}